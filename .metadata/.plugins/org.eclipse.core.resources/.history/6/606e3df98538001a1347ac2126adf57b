package movement;
import java.util.Scanner;

public class MovementOfPosition {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int x=0;
		int y=0;
		char c=sc.next().charAt(0);
		while(c!=0)
		{
			if(c=='L')
				x-=1;
			else if(c=='R')
			     x+=1;
			else if(c=='D')
				y-=1;
			else if(c=='U')
				y+=1;
			
			
			
		}
		System.out.print(x+" "+y);
		
		
	}

}
