package BST;

import java.util.Scanner;

public class KthSmallestElement {
	static Node root;
	static class Node {
		int data;
		Node left,right;
		Node(int d){
			data=d;
		}
	}
	static Node build(Node root,int data) {
		if(root==null) {
			root=new Node(data);
			return root;
		}
		if(data>root.data) {
			root.right=build(root.right,data);
		} else if(data<root.data) {
			root.left=build(root.left,data);
		}
		return root;
	}
	static Node print(Node root,int k,int count) {
		
		if(root!=null) {
		Node left =print(root.left,k,count);
			if(count++==k)
				System.out.println(root.data);
			if(left!=null) return left;
			
		Node right=print(root.right,k,count);	
		}
		return root;
		
	}
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int T=sc.nextInt();
		while(T-->0) {
			build(root,sc.nextInt());
		}
		int k=sc.nextInt();
		print(root,k,0);
		
	}

}
